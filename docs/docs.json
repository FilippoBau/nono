      },
      {
        "group": "Agents & Clients",
        "pages": [
          "cli/clients/quickstart",
          "cli/clients/claude-code",
          "cli/clients/opencode",
          "cli/clients/openclaw"
        ]
      },
      {
        "group": "Security",
        "pages": [
          "cli/security/index",
          "cli/security/profiles",
          "cli/security/signing",
          "cli/security/containers",
          "cli/security/landlock",
          "cli/security/seatbelt",
          "cli/security/application"
        ]
      },
      {
        "group": "Reference",
        "pages": ["cli/usage/flags"]
