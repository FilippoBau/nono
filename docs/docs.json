      {
        "group": "Development",
        "pages": ["cli/development/index", "cli/development/testing"]
      }
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/always-further/nono",
      "discord": "https://discord.gg/pPcjYzGvbS"
    }
